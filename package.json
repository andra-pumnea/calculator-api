{
  "name": "serverless-nodejs-app",
  "version": "1.0.0",
  "main": "index.js",
  "dependencies": {
    "@types/express": "^4.17.6",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "serverless-http": "^2.4.1",
    "ts-node": "^8.10.1",
